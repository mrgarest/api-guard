{
    "name": "garest/api-guard",
    "description": "A lightweight Laravel library for authenticating API clients without using user models",
    "type": "library",
    "license": "MIT",
    "version": "1.1.0",
    "homepage": "https://github.com/mrgarest/api-guard",
    "keywords": [
        "Garest",
        "MrGarest",
        "api",
        "token",
        "auth",
        "hmac",
        "laravel",
        "php"
    ],
    "authors": [
        {
            "name": "Garest",
            "homepage": "https://github.com/mrgarest"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Garest\\ApiGuard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Garest\\ApiGuard\\Providers\\ApiGuardProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
